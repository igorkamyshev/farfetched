{
  "name": "@farfetched/typed-contracts",
  "version": "0.11.2",
  "peerDependencies": {
    "typed-contracts": "^3.0.0",
    "@farfetched/core": "workspace:*"
  },
  "scripts": {
    "test:run": "vitest run",
    "typetest:run": "vitest typecheck --run",
    "build": "vite build",
    "size": "size-limit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/typed-contracts.cjs",
  "module": "./dist/typed-contracts.js",
  "exports": {
    ".": {
      "import": "./dist/typed-contracts.js",
      "require": "./dist/typed-contracts.cjs"
    }
  },
  "size-limit": [
    {
      "path": "./dist/typed-contracts.js",
      "limit": "361 B"
    }
  ]
}
