{
  "name": "@farfetched/solid",
  "version": "0.11.2",
  "dependencies": {},
  "peerDependencies": {
    "effector-solid": "^0.23.0",
    "solid-js": "^1.4.7"
  },
  "scripts": {
    "test:run": "vitest run",
    "typetest:run": "vitest typecheck --run",
    "build": "vite build",
    "size": "size-limit",
    "publint": "node ../../tools/scripts/publint.mjs",
    "typelint": "attw --pack"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/solid.cjs",
  "module": "./dist/solid.js",
  "types": "./dist/solid.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/solid.d.ts",
        "default": "./dist/solid.js"
      },
      "require": {
        "types": "./dist/solid.d.cts",
        "default": "./dist/solid.cjs"
      }
    }
  },
  "size-limit": [
    {
      "path": "./dist/solid.js",
      "limit": "571 B"
    }
  ]
}
