{
  "name": "@farfetched/zod",
  "version": "0.11.2",
  "peerDependencies": {
    "zod": "^3.19",
    "@farfetched/core": "workspace:*"
  },
  "scripts": {
    "test:run": "vitest run",
    "typetest:run": "vitest typecheck --run",
    "build": "vite build",
    "size": "size-limit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/zod.cjs",
  "module": "./dist/zod.js",
  "types": "./dist/zod.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/zod.d.ts",
        "default": "./dist/zod.js"
      },
      "require": {
        "types": "./dist/zod.d.cts",
        "default": "./dist/zod.cjs"
      }
    }
  },
  "size-limit": [
    {
      "path": "./dist/zod.js",
      "limit": "231 B"
    }
  ]
}
