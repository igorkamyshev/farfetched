{
  "name": "@farfetched/io-ts",
  "version": "0.11.2",
  "peerDependencies": {
    "io-ts": "^2.2.20",
    "fp-ts": "^2.13.1",
    "@farfetched/core": "workspace:*"
  },
  "scripts": {
    "test:run": "vitest run",
    "typetest:run": "vitest typecheck --run",
    "build": "vite build",
    "size": "size-limit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/io-ts.cjs",
  "module": "./dist/io-ts.js",
  "types": "./dist/io-ts.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/io-ts.d.ts",
        "default": "./dist/io-ts.js"
      },
      "require": {
        "types": "./dist/io-ts.d.cts",
        "default": "./dist/io-ts.cjs"
      }
    }
  },
  "size-limit": [
    {
      "path": "./dist/io-ts.js",
      "limit": "174 B"
    }
  ]
}
