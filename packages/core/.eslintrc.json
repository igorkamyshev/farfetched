{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.spec.ts"],
      "rules": { "@nrwl/nx/enforce-module-boundaries": "off" }
    },
    {
      "files": ["*.type_spec.ts"],
      "rules": {
        "no-unused-labels": "off",
        "@typescript-eslint/no-unused-vars": "off"
      }
    }
  ]
}
