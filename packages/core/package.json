{
  "name": "@farfetched/core",
  "version": "0.11.2",
  "peerDependencies": {
    "effector": "^23.0.0"
  },
  "scripts": {
    "test:run": "vitest run",
    "typetest:run": "vitest typecheck --run",
    "build": "vite build",
    "size": "size-limit"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      },
      "require": {
        "types": "./dist/core.d.cts",
        "default": "./dist/core.cjs"
      }
    }
  },
  "size-limit": [
    {
      "path": "./dist/core.js",
      "limit": "15.7 kB"
    }
  ]
}
